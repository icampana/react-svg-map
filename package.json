{
	"name": "react-svg-map",
	"version": "2.2.0",
	"description": "A set of React.js components to display an interactive SVG map",
	"main": "lib/index.js",
	"scripts": {
		"start": "webpack-dev-server --mode development --open --config webpack.examples.config.js",
		"build": "webpack --mode production",
		"lint": "eslint ./src/*.js*",
		"test": "jest",
		"test:update": "jest --updateSnapshot",
		"build-demo": "webpack --mode production --config webpack.examples.config.js && cp examples/dist/* docs",
		"prepare": "yarn lint && yarn test && yarn build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/VictorCazanave/react-svg-map.git"
	},
	"keywords": [
		"react",
		"component",
		"svg",
		"interactive",
		"map",
		"checkbox",
		"radiobutton"
	],
	"author": "Victor Cazanave <victor.cazanave@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/VictorCazanave/react-svg-map/issues"
	},
	"homepage": "https://github.com/VictorCazanave/react-svg-map#readme",
	"files": [
		"/src",
		"/lib"
	],
	"devDependencies": {
        "@babel/core": "^7.24.7",
        "@babel/eslint-parser": "^7.24.7",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/preset-env": "^7.24.7",
        "@babel/preset-react": "^7.24.7",
		"@svg-maps/australia": "^1.1.1",
		"@svg-maps/france.regions": "^1.1.1",
		"@svg-maps/taiwan.main": "^1.0.0",
		"@svg-maps/usa": "^1.1.1",
		"@svg-maps/usa.utah": "^1.1.1",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.5.2",
        "@testing-library/dom": "^10.1.0",
		"babel-jest": "^29.7.0",
		"babel-loader": "^9.1.3",
		"css-loader": "^7.1.2",
		"eslint": "^8.57.0",
		"eslint-plugin-jest": "^28.6.0",
		"eslint-plugin-react": "^7.34.3",
		"eslint-webpack-plugin": "^4.2.0",
		"html-loader": "^5.0.0",
